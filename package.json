{
  "name": "storelib-ts",
  "version": "1.0.2",
  "description": "A StoreLib rewrite in TypeScript.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "build": "tsup src/index.ts --format cjs,esm --minify --clean --dts",
    "lint": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "packageManager": "pnpm@10.0.0",
  "devDependencies": {
    "@types/he": "^1.2.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@types/xmldom": "^0.1.34",
    "he": "^1.2.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.34"
  }
}